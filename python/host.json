{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  },
  "extensions": {
    "http": {
        "routePrefix": ""
    }
  },
  "functionTimeout": "08:00:00",
  "logging": {
    "logLevel": {
      "default": "Information",
      "Host.Results": "Error",
      "Host.Aggregator": "Trace",
      "Function": "Trace",
      "Function.api": "Information",
      "Function.importer": "Information"
    },
    "tracing": {
      "consoleLevel": "verbose"
    }
  }
}